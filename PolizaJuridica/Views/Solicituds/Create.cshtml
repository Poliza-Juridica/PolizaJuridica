                                <div class="form-group label-floating" id="AlcaldiaMunicipioArrendarError">
                                    <label class="control-label"><star>*</star>Alcaldía o Municipio</label>
                                    <input asp-for="AlcaldiaMunicipioArrendar" type="text" class="form-control">
