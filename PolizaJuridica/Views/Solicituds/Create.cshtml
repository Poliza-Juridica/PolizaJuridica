@model PolizaJuridica.Data.Solicitud

<div class="row">
    <!--      Wizard container        -->
    <div class="wizard-container">
        <div class="card wizard-card form" data-color="red" id="wizardProfile">
            <form asp-controller="Solicituds" asp-action="Create">
                <div align="center">
                    <h3 class="wizard-title">
                        Solicitud de Otorgamiento
                    </h3>
                    <!--<div class="row">-->
                    <div class="radio" id="SolicitudTipoPolizaError">
                        <label><star>*</star>Tipo de p&oacuteliza: </label>
                        <label>
                            <input asp-for="SolicitudTipoPoliza" type="radio" value="Tradicional">
                            Tradicional
                        </label>
                        <label>
                            <input asp-for="SolicitudTipoPoliza" type="radio" value="Plus">
                            Plus
                        </label>
                    </div>
                    <!--</div>-->
                </div>
                <div class="wizard-navigation">
                    <ul>
                        <li>
                            <a href="#account" data-toggle="tab">Datos del Arrendador</a>
                        </li>
                        <li>
                            <a href="#address" data-toggle="tab">Datos del Arrendamiento</a>
                        </li>
                        <li>
                            <a href="#about" data-toggle="tab">Datos del Profesional inmobiliario</a>
                        </li>
                    </ul>
                </div>
                <div class="tab-content">
                    <div class="tab-pane" id="about">

                        @if (@ViewBag.Area != "Representante")
                        {
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group label-floating" hidden>
                                        <label class="control-label">RepresentanteId</label>
                                        <input asp-for="Representanteid" type="text" class="form-control">
                                    </div>
                                    <div class="col-md-5">
                                        <div class="form-group">
                                            <label class="control-label">Representante</label>
                                            <input name="RepresentanteNombre" id="RepresentanteNombre" type="text" class="form-control" disabled>
                                        </div>
                                    </div>
                                    <div class="col-md-5">
                                        <div class="form-group">
                                            <label class="control-label">Representación</label>
                                            <input id="NombreRepresentacion" type="text" class="form-control" disabled>
                                        </div>
                                    </div>
                                    <div class="col-md-2">
                                        <a class="btn btn-fill btn-primary btn-wd" href="" data-toggle="modal" data-target="#Representantes"><i class="material-icons">search</i></a>
                                    </div>
                                </div>
                            </div>

                        }
                        @if (@ViewBag.Area != "Asesor")
                        {
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group label-floating" hidden>
                                        <label class="control-label">Asesor</label>
                                        <input asp-for="Asesorid" type="text" class="form-control">
                                    </div>
                                    <div class="col-md-5">
                                        <div class="form-group">
                                            <label class="control-label">Asesor</label>
                                            <input id="AsesorNombre" name="AsesorNombre" type="text" class="form-control" disabled>
                                        </div>
                                    </div>
                                    <div class="col-md-5">
                                        <div class="form-group">
                                            <label class="control-label">Inmobiliaria</label>
                                            <input asp-for="Inmobiliaria" type="text" class="form-control" disabled>
                                        </div>
                                    </div>
                                    <div class="col-md-2">
                                        <a class="btn btn-fill btn-primary btn-wd" href="" data-toggle="modal" data-target="#Asesor"><i class="material-icons">search</i></a>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label class="control-label">Teléfono</label>
                                            <input asp-for="SolicitudTelefono" type="text" class="form-control">
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label class="control-label">Celular</label>
                                            <input asp-for="SolicitudCelular" type="text" class="form-control">
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label class="control-label">E-mail</label>
                                            <input asp-for="SolicitudEmail" type="email" class="form-control" disabled>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        }

                        <div class="row">
                            <div class="col-md-4">
                                <div class="checkbox">
                                    <label>Es propietario: </label>
                                    <label>
                                        <input asp-for="SolicitudAdmiInmueble">
                                    </label>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="checkbox">
                                    <label>Es administrador de inmuebles: </label>
                                    <label>
                                        <input asp-for="SolicitudEsAdminInmueble">
                                    </label>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="checkbox">
                                    <label>Es renovación: </label>
                                    <label>
                                        <input asp-for="IsRenovacion">
                                    </label>
                                </div>
                            </div>
                        </div>

                    </div>
                    <div class="tab-pane" id="account">
