        var UsuarioNombre;
        var UsuarioApellidoPaterno;
        var UsuarioApellidoMaterno;
        var UsuarioCelular;
        var UsuarioTelefono;
        var UsurioEmail;
        var UsuarioInmobiliaria;
        var UsuarioIdR;
        var ResesentanteId;
        /*Insertamos un registro*/
        function setDataAdd(action) {

            UsuarioNombre = $('input[name=UsuarioNombre')[0].value;
            UsuarioApellidoPaterno = $('input[name=UsuarioApellidoPaterno')[0].value;
            UsuarioApellidoMaterno = $('input[name=UsuarioApellidoMaterno')[0].value;
            UsuarioCelular = $('input[name=UsuarioCelular')[0].value;
            UsuarioTelefono = $('input[name=UsuarioTelefono')[0].value;
            UsurioEmail = $('input[name=UsurioEmail')[0].value;
            UsuarioInmobiliaria = $('input[name=UsuarioInmobiliaria')[0].value;
            //ResesentanteId = $('input[name=Representanteid')[0].value;
            $.ajax({
                type: "POST",
                url: "../../" + action,
                data: {
                    UsuarioNombre, UsuarioApellidoPaterno, UsuarioApellidoMaterno, UsuarioTelefono, UsurioEmail, UsuarioInmobiliaria, UsuarioCelular, ResesentanteId
                },
                success: function (response) {
                    var result = JSON.parse(response);
                    for (var i in result) {

                        switch (result[i].tipo) {
                            case 1://Error
                                Mensaje(result[i].mensaje, "danger");
                                break;
                            case 2://ErrorAtributos
                                MensajeCampos(result[i].mensaje);
                                break;
                            case 3://Exitoso
                                $("#NuevoAsesor").modal('toggle');
                                Mensaje(result[i].mensaje, "success");
                                var NombreCompleto = UsuarioNombre + " " + UsuarioApellidoPaterno + UsuarioApellidoMaterno;
                                $("#Asesorid").val(response);
                                $("#AsesorNombre").val(NombreCompleto);
                                $("#Inmobiliaria").val(UsuarioInmobiliaria);
                                $("#SolicitudTelefono").val(UsuarioTelefono);
                                $("#SolicitudCelular").val(UsuarioCelular);
                                $("#SolicitudEmail").val(UsurioEmail);
                                $("#Asesor").modal('toggle');
                                break;
                            case 4://Informativo
                                Mensaje(result[i].mensaje, "info");
                                break;
                        }

                    }
                }
            });
        }

        var objeto = @Html.Raw(Json.Serialize(@ViewBag.Error));
        if (objeto != null) {
            var obj = JSON.parse(objeto);
            navegar(obj);
        }

        function navegar(obj) {

            if (obj != null) {
                for (var o in obj) {

                    switch (obj[o].tipo) {
                        case 1://Error
                        Mensaje(obj[o].mensaje, "danger");
                        break;
                        case 2://ErrorAtributos
                        MensajeCampos(obj[o].mensaje);
                        break;
                        case 3://Exitoso
                        Mensaje(obj[o].mensaje, "success");
                        break;
                        case 4://Informativo
                        Mensaje(obj[o].mensaje, "info");
                        break;
                    }

                }
            }
        }

    function Mensaje(mensaje, tipo) {
    $.notify({
            icon: "add_alert",
        message: mensaje

    }, {
            type: tipo,
            timer: 10000,
            placement: {
                from: "top",
                align: "right"
            }
        });
    }
       function MensajeCampos(id) {
           id = '#' + id;
            $(id).addClass('has-error');
        }


