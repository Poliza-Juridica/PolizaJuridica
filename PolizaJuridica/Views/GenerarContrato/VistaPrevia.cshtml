                                                                            </td>
                                                                            <td>
                                                                                @c.DocumentoPlantillaId
                                                                            </td>
                                                                            <td>
                                                                                @c.DocumentoPlantillaNombre
                                                                            </td>
                                                                            <td>
                                                                                <a asp-action="Generar" asp-route-id="@ViewBag.Id" asp-route-contratoid="@c.DocumentoPlantillaId">Generar</a>
                                                                            </td>
                                                                        </tr>
                                                                    }
                                                                </tbody>
                                                            </table>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <!-- end col-md-12  fin de contenedor-->
                            </div>
                        </div>
                    </div>
                </div>
                <!--Fin de Seccion 1-->
            </div>
        </div>
    </div>
</div>
    </div>
</div>
<!-- end row -->
@section Scripts   {

<script type="text/javascript">
        var objeto = @Html.Raw(Json.Serialize(@ViewBag.Error));
        if (objeto != null) {
            var obj = JSON.parse(objeto);
            navegar(obj);
        }

        function navegar(obj) {

            if (obj != null) {
                for (var o in obj) {

                    switch (obj[o].tipo) {
                        case 1://Error
                        Mensaje(obj[o].mensaje, "danger");
                        break;
                        case 2://ErrorAtributos
                        MensajeCampos(obj[o].mensaje);
                        break;
                        case 3://Exitoso
                        Mensaje(obj[o].mensaje, "success");
                        break;
                        case 4://Informativo
                        Mensaje(obj[o].mensaje, "info");
                        break;
                    }

                }
            }
        }

    function Mensaje(mensaje, tipo) {
    $.notify({
            icon: "add_alert",
        message: mensaje

    }, {
            type: tipo,
            timer: 10000,
            placement: {
                from: "top",
                align: "right"
            }
        });
    }
       function MensajeCampos(id) {
           id = '#' + id;
            $(id).addClass('has-error');
        }
        function setDataJurrisdiccion(id,action) {
            jurisdiccion = $('input[name=jurisdiccion')[0].value;

            $.ajax({
                type: "POST",
                url: "../../" + action,
                data: {
                    id, jurisdiccion
                },
                success: function (response) {
                    var result = JSON.parse(response);
                    for (var i in result) {

                        switch (result[i].tipo) {
                            case 1://Error
                                Mensaje(result[i].mensaje, "danger");
                                break;
                            case 2://ErrorAtributos
                                MensajeCampos(result[i].mensaje);
                                break;
                            case 3://Exitoso
                                Mensaje(result[i].mensaje, "success");
                                break;
                            case 4://Informativo
                                Mensaje(result[i].mensaje, "info");
                                break;
                        }

                    }
                }
            });
        }
        //Inicio datatablesContratos
            $('#datatablesContratos').DataTable({
                "pagingType": "full_numbers",
                "lengthMenu": [
                    [10, 25, 50, -1],
                    [10, 25, 50, "Todo"]
                ],
                responsive: true,
                language: {
                    search: "_INPUT_",
                    searchPlaceholder: "Buscar",
                }

            });


            var table = $('#datatablesContratos').DataTable();

            // Edit record
            table.on('click', '.edit', function () {
                $tr = $(this).closest('tr');

                var data = table.row($tr).data();
                alert('You press on Row: ' + data[0] + ' ' + data[1] + ' ' + data[2] + '\'s row.');
            });

            // Delete a record
            table.on('click', '.remove', function (e) {
                $tr = $(this).closest('tr');
                table.row($tr).remove().draw();
                e.preventDefault();
            });

            //Like record
            table.on('click', '.like', function () {
                alert('You clicked on Like button');
            });



            $('.card .material-datatables label').addClass('form-group');



</script>
}


