                                    <!--INICIO Seccion dentro del tab-->
                                    <div class="row">
                                        <!--Seccion de contenedor-->
                                        <div class="col-lg-4 col-md-3">
                                            <div class="card card-pricing card-raised">
                                                <div class="content">
                                                    <h3 class="card-title">Jurisdicción</h3>
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="form-group " id="jurisdiccionError">
                                                                <label class="control-label"><star>*</star>Jurisdicción</label>
                                                                <input name="jurisdiccion" class="form-control" />
                                                            </div>
                                                        </div>
                                                    </div>
