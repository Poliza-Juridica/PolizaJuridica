                                                    <div class="row">
                                                        <div class="material-datatables">
                                                            <table id="datatablesContratos" class="table table-striped table-no-bordered table-hover" cellspacing="0" width="100%" style="width:100%">
                                                                <thead>
                                                                    <tr>
                                                                        <th>
                                                                            Tipo
                                                                        </th>
                                                                        <th>
                                                                            ID
                                                                        </th>
                                                                        <th>
                                                                            Nombre
                                                                        </th>
