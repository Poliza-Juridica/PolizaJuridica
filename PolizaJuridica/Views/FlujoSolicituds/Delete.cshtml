@model PolizaJuridica.Data.FlujoSolicitud

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var Id = Model.SolicitudId;
}

<h2>Eliminar</h2>

<h3>¿Estas seguro de eliminar a la siguiente persona del flujo?</h3>
<div>
    <h4>FlujoSolicitud</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.FechaAsignacion)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.FechaAsignacion)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Persona)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Persona.UsuarioNomCompleto)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Solicitud)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Solicitud.SolicitudId)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Delete" class="btn btn-default" /> |
        <a asp-action="Index" asp-route-Id="@Id">Regresar</a>
    </form>
</div>
