                        <div class="col-md-6 col-md-offset-1">
                            <div class="table-responsive table-sales">
                                <table class="table">
                                    <tbody>
                                    <td class="text-center">
                                        Id
                                    </td>
                                    <td class="text-center">
                                        Nombre
                                    </td>
                                    <td class="text-center">
                                        Cantidad
                                    </td>
                                    @foreach (var u in ViewData["CPU"] as List<UsuarioCantidadViewModel>)
                                    {
                                        <tr>
                                            <td class="text-center">
                                                @u.Id
