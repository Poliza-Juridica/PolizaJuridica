@using PolizaJuridica.ViewModels;
    <div class="card">
        <div class="card-header">
            <div class="row">
                <h3>@ViewBag.FiltroFecha</h3>
            </div>
            <div class="row">
                @using (Html.BeginForm())
                {
                    <div class="col-md-5">
                        <div class="form-group">
                            <label class="control-label">Fecha Inicio </label>
                            <input name="FechaInicio" class="form-control" type="date">
                        </div>
                    </div>
                    <div class="col-md-5">
                        <div class="form-group">
                            <label class="control-label">Fecha Fin </label>
                            <input name="FechaFin" class="form-control" type="date">
                        </div>
                    </div>
                    <div class="col-md-2">
                        <div class="form-group">
                            <input type="submit" class='btn btn-fill btn-primary btn-wd' value="Filtrar">
                        </div>
                    </div>

                }

            </div>
        </div>
        <div class="card-content">
            <ul class="nav nav-pills nav-pills-warning">
                <li class="active">
                    <a href="#pill1" data-toggle="tab">Vista General</a>
                </li>
                <li>
                    <a href="#pill7" data-toggle="tab">Representaciones</a>
                </li>
                <li>
                    <a href="#pill2" data-toggle="tab">Atención a Clientes</a>
                </li>
                <li>
                    <a href="#pill3" data-toggle="tab">Procesos</a>
                </li>
                <li>
                    <a href="#pill4" data-toggle="tab">Cuentas por Pagar</a>
                </li>
                <li>
                    <a href="#pill5" data-toggle="tab">Cuentas por Cobrar</a>
                </li>
                <li>
                    <a href="#pill6" data-toggle="tab">Soluciones</a>
                </li>

            </ul>
            <div class="tab-content">
                <div class="tab-pane active" id="pill1">
                    <div class="card-content">
                        <h4 class="card-title">Estatus Procesos</h4>
                        <div class="row">
                            <div class="col-md-5">
                                <div class="table-responsive table-sales">
                                    <table class="table">
                                        <tbody>
                                            <tr>
                                                <td>
                                                    Cuenta
                                                </td>
                                                <td>
                                                    Saldo
                                                </td>
                                            </tr>
                                            @foreach (var c in ViewData["Cuentas"] as IList<CuentasBancarias>)
                                            {
                                                <tr>
                                                    <td>@c.Nombre</td>
                                                    <td class="text-right">
                                                        @c.Saldo
                                                    </td>
                                                </tr>
                                            }
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                            <div class="col-md-5">
                                <div class="table-responsive table-sales">
                                    <table class="table">
                                        <tbody>
                                            <tr>
                                                <td>
                                                    Estatus
                                                </td>
                                                <td>Cantidad</td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    Recibidas
                                                </td>
                                                <td>@ViewBag.Recibidas</td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    Solicitudes en Atención a Clientes
                                                </td>
                                                <td>@ViewBag.AtencionClientes</td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    Solicitudes en Procesos
                                                </td>
                                                <td>@ViewBag.Procesos</td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    Total de Solicitudes Concluidas
                                                </td>
                                                <td>@ViewBag.SolicitudConcluida</td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    Total de solicitudes Canceladas
                                                </td>
                                                <td>@ViewBag.Canceladas</td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    Total de Pólizas
                                                </td>
                                                <td>@ViewBag.Polizas</td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    Total de Solicitudes
                                                </td>
                                                <td>@ViewBag.TotalSolicitudes</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
