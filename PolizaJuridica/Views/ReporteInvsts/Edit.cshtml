@model PolizaJuridica.Data.ReporteInvst

@{
    ViewData["Title"] = "Actualizar";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var Arr = ViewBag.Arrendatario as FisicaMoral;

}
<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-header card-header-icon" data-background-color="purple">
                <i class="material-icons">assignment</i>
            </div>
            <div class="card-content">
                <h4 class="card-title">Actualizar </h4>
                <div class="material-datatables">
                    <div class="row">
                        <div class="col-md-4">
                            <form asp-action="Edit">
                                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                                <div class="form-group">
                                    <label asp-for="Texto1" class="control-label"></label>
                                    <textarea asp-for="Texto1" class="form-control" rows="5"></textarea>
                                    <span asp-validation-for="Texto1" class="text-danger"></span>
                                </div>
                                <div class="form-group">
                                    <label asp-for="Texto2" class="control-label"></label>
                                    <textarea asp-for="Texto2" class="form-control" rows="5"></textarea>
                                    <span asp-validation-for="Texto2" class="text-danger"></span>
                                </div>
                                <div class="form-group" hidden>
                                    <input asp-for="FisicaMoralId" class="form-control" />
                                </div>
                                <div class="form-group">
                                    <a class="btn btn-default" asp-controller="Solicituds" asp-action="Index">Cancelar</a>
                                    <input type="submit" value="Actualizar" class="btn btn-primary" />
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-header card-header-icon" data-background-color="purple">
                <i class="material-icons">assignment</i>
            </div>
            <div class="card-content">
                <h4 class="card-title">Datos del Arrendatario </h4>
                <div class="material-datatables">
                    @if (Arr.Solicitud.TipoArrendatario == "1")
                    {
                        @:<div class="row">
                            @:<div class="col-md-4">
                                <label>Nombre: @Arr.SfisicaNombre @Arr.SfisicaApePat @Arr.SfisicaApeMat</label>
                                <br />
                                <label>Nacionalidad: @Arr.SfisicaNacionallidad</label>
                                <br />
                                <label>Estado Civil: @Arr.SfisicaEstadoCivil</label>
                                @:</br>
                                <label>Celular: @Arr.SfisicaCelular</label>
                                @:</br>
                                <label>Teléfono: @Arr.SfisicaTelefono</label>
                                @:</br>
                                <label>E-mail: @Arr.SfisicaEmail</label>
                            @:</div>
                            <div class="col-md-4">
                                <label>@Arr.SfisicaDomicilio</label>
                                <br />
                                <label>Colonia: @Arr.SfisicaColonia</label>
                                <br />
                                <label>Alcaldía / Municipio:@Arr.SfisicaDelegacionMunicipio</label>
                            </div>

                            @:<div class="col-md-4">
                                <label>Ingreso Mensual: @Arr.SfisicaIngresoMensual</label>
                                @: </br>
                                <label>Empresa: @Arr.SfisicaTrabajo</label>
                                @:</br>
                                <a class="btn btn-default" asp-controller="Solicituds" asp-action="Edit" asp-route-id="@Arr.FisicaMoralId">Editar</a>
                            @:</div>
                        @:</div>
                    }
                    @if (Arr.Solicitud.TipoArrendatario == "2")
                    {
                        @:<div class="row">
                            @:<div class="col-md-4">
                                <label>Razón social: @Arr.SfisicaRazonSocial</label>
                                <br />
                                <label>RFC: @Arr.SfisicaRfc</label>
                                <br />
                                <label>Nombre: @Arr.SfisicaNombre @Arr.SfisicaApePat @Arr.SfisicaApeMat</label>
                                <br />
                                <label>Nacionalidad: @Arr.SfisicaNacionallidad</label>
                                <br />
                                <label>Estado Civil: @Arr.SfisicaEstadoCivil</label>
                                @:</br>
                                <label>Celular: @Arr.SfisicaCelular</label>
                                @:</br>
                                <label>Teléfono: @Arr.SfisicaTelefono</label>
                                @:</br>
                                <label>E-mail: @Arr.SfisicaEmail</label>
                            @:</div>
                            <div class="col-md-4">
                                <label>@Arr.SfisicaDomicilio</label>
                                <br />
                                <label>Colonia: @Arr.SfisicaColonia</label>
                                <br />
                                <label>Alcaldía / Municipio:@Arr.SfisicaDelegacionMunicipio</label>
                            </div>

                            @:<div class="col-md-4">
                                <label>Ingreso Mensual: @Arr.SfisicaIngresoMensual</label>
                                @:</br>
                                <label>Empresa: @Arr.SfisicaTrabajo</label>
                                @:</br>
                                <a class="btn btn-default" asp-controller="Solicituds" asp-action="Edit" asp-route-id="@Arr.FisicaMoralId">Editar</a>
                            @:</div>
                        @:</div>
                    }
                </div>
            </div>
        </div>
    </div>
</div>

<div class="row">
