        function setDataDeleteFiadorF(action) {

            id = $('input[name=FiadorMId')[0].value;


            $.each(item, function (idex, valor) {
                valor.FiadorFId = id;
            });

            $.ajax({
                type: "POST",
                url: "../../" + action,
                data: {
                    id
                },
                success: function (response) {
                    if (response == "Save") {
                        swal({
                            title: 'Eliminado!',
                            text: 'Su registro ha sido eliminado.',
                            type: 'success',
                            confirmButtonClass: "btn btn-success",
                            buttonsStyling: false
                        })
                        setTimeout(function () {
                            window.location.reload(true);
                        }, 2000);
                    }
                    else {
                        //Mesaje de error
                        $.notify({
                            icon: "notifications",
                            message: "Error," + response

                        }, {
                            type: danger[red],
                            timer: 3000,
                            placement: {
                                from: top,
                                align: right
                            }
                        });
                    }
                }
            });
        }
        function MensajeError() {
            swal({
                title: '¿Estas Seguro?',
                text: "Se eliminara de forma permanente",
                type: 'warning',
                showCancelButton: true,
                confirmButtonClass: 'btn btn-success',
                cancelButtonClass: 'btn btn-danger',
                confirmButtonText: 'Si, Eliminarlo!',
                buttonsStyling: false
            }).then(function () {
                setDataDeleteFiadorF('FiadorMs/DeleteConfirmed');
            });
        }
        function Mensaje(mensaje, tipo) {
            $.notify({
                icon: "add_alert",
                message: mensaje

            }, {
                type: tipo,
                timer: 1000,
                placement: {
                    from: "top",
                    align: "right"
                }
            });
        }
        function MensajeCampos(id) {
            id = '#' + id;
            $(id).addClass('has-error');
