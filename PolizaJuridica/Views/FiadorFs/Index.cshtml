                    if (response == "Save") {
                        swal({
                            title: 'Eliminado!',
                            text: 'Su registro ha sido eliminado.',
                            type: 'success',
                            confirmButtonClass: "btn btn-success",
                            buttonsStyling: false
                        })
                        setTimeout(function () {
                            window.location.reload(true);
                        }, 2000);
                    }
                    else {
                        //Mesaje de error
                        $.notify({
                            icon: "notifications",
                            message: "Error," + response

                        }, {
                            type: "danger",
                            timer: 3000,
                            placement: {
                                from: top,
                                align: "right"
                            }
                        });
                    }
                }
            });
        }
