            $.ajax({
                type: "POST",
                url: "../../" + action,
                data: {
                    FiadorFId, FiadorFNombres, FiadorFApePaterno, FiadorFApeMaterno, FiadorFNacionalidad, FiadorFCondicionMigratoria, FiadorFParentesco, FiadorFEstadoCivil, FiadorFConvenioEC, FiadorFDomicilio, FiadorFColonia, FiadorFDelegacion, FiadorFTelefono, FiadorFCelular, FiadorFEmail, FiadorFDomicilioGarantia, FiadorFColoniaGarantia, FiadorFDelegaciónGarantia, FiadorFProfesion, FiadorFEmpresa, FiadorFTelefonoEmpresa, FiadorFNombresConyuge, FiadorFApePaternoConyuge, FiadorFApeMaternoConyuge, FisicaMoralId, FiadorFEstado, FiadorFEstadoGarantia, FiadorFCodigoPostal, FiadorFCodigoPostalGarantia, EscrituraNumero, Licenciado, NumeroNotaria, DistritoJudicial, PartidaNumero, PartidaVolumen, PartidaLibro, PartidaSeccion, PartidaFecha, TipoIdentificacion, NumeroIdentificacion, TipoInmobiliarioId
                },
                success: function (response) {
                    var result = JSON.parse(response);
                    for (var i in result) {

                        switch (result[i].tipo) {
                            case 1://Error
                                Mensaje(result[i].mensaje, "danger");
                                break;
                            case 2://ErrorAtributos
                                MensajeCampos(result[i].mensaje);
                                break;
                            case 3://Exitoso
                                $("#Create").modal('toggle');
                                Mensaje(result[i].mensaje, "success");
                                window.location.reload(true);
                                break;
                            case 4://Informativo
                                Mensaje(result[i].mensaje, "info");
                                break;
                        }

                    }
                }
            });
