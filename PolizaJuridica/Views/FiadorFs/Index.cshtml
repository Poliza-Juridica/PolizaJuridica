        function Mensaje(mensaje, tipo) {
            $.notify({
                icon: "add_alert",
                message: mensaje

            }, {
                type: tipo,
                timer: 10000,
                placement: {
                    from: "top",
                    align: "right"
                }
            });
        }
        function MensajeCampos(id) {
            id = '#' + id;
